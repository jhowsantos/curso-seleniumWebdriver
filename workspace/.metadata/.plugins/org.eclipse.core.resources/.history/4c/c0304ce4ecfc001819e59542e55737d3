package main;

public class Gerente {
	private String nome;
	private boolean podeDarPromocao;
	
	public Gerente(String nome, boolean podeDarPromocao){
		this.nome = nome;
		this.podeDarPromocao = podeDarPromocao;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public boolean isPodeDarPromocao() {
		return podeDarPromocao;
	}

	public void setPodeDarPromocao(boolean podeDarPromocao) {
		this.podeDarPromocao = podeDarPromocao;
	}
	
	
}
